{
  "name": "@hexlet/code",
  "version": "1.0.0",
  "description": "task manager",
  "main": "server/index.js",
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --bail",
    "prebuild": "rm -rf dist",
    "build-front": "NODE_ENV=production webpack --mode production",
    "build-server": "NODE_ENV=production babel server --out-dir dist/server --source-maps inline --copy-files",
    "build": "npm run build-server && npm run build-front",
    "start": "NODE_ENV=production node dist/server/bin/server.js",
    "dev-build-front": "webpack",
    "devbuild": "npm run dev-build-front",
    "devstart": "nodemon --exec babel-node server/bin/server.js",
    "heroku": "heroku local -f Procfile.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nidges/backend-project-lvl4.git"
  },
  "author": "nidges",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nidges/backend-project-lvl4/issues"
  },
  "homepage": "https://github.com/nidges/backend-project-lvl4#readme",
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.7",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.8",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.3.4",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "fastify": "^3.25.3",
    "mini-css-extract-plugin": "^2.4.6",
    "point-of-view": "^5.0.0",
    "pug": "^3.0.2",
    "webpack": "^5.65.0"
  }
}
